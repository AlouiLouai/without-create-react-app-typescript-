{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "esnext",
            "ES6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strict": true,
        "sourceMap": true,
        "target": "esnext",
    },
    "exclude": [
        "node_modules"
    ]
}